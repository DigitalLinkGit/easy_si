{% extends 'base.html.twig' %}

{% block title %}Capture
{% endblock %}

{% block body %}
	{% include 'global/partials/_page_header.html.twig' with {
    route_return: 'app_capture_index',
    title: 'Capture :'
    } %}

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ capture.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ capture.name }}</td>
			</tr>
			<tr>
				<th>Description</th>
				<td>{{ capture.description }}</td>
			</tr>
		</tbody>
	</table>
{% endblock %}
