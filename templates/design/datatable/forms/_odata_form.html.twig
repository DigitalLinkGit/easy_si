{# templates/manage_data/forms/_odata_form.html.twig #}

{{ form_start(form) }}
    <div class="row mb-3">
        <div class="col-12">{{ form_row(form.url) }}</div>
    </div>
    <div class="row align-items-end mb-3">
        <div class="col-md-3">{{ form_row(form.username) }}</div>
        <div class="col-md-3">{{ form_row(form.password) }}</div>
        <div class="col-md-3 offset-md-3 text-end">{{ form_row(form.submit) }}</div>
    </div>
{{ form_end(form) }}
